/* components/playlist/playlist.wxss */
.cards{
  width: 220rpx;
  position: relative;
  padding-bottom: 20rpx;
}
.card-images{
  height: 220rpx;
  width: 100%;
  border-radius: 15rpx;
}
.play-playcount{
  font-size: 24rpx;
  color: #ffffff;
  text-shadow: 1px 0 0 rgba(0,0,0,0.15);
  position: absolute;
  right: 10rpx;
  top: 4rpx;
  padding-left: 26rpx;
  background:url(data:image/png;base64,) no-repeat 0 8rpx/22rpx 20rpx;
}
.cards-title{
  font-size: 26rpx;
  line-height: 1.2;
  padding:4rpx 0 0 12rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}